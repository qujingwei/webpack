{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "webpack",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix --ext .js src",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --config webpack.dev.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "qujinwei",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^6.0.0",
    "lint-staged": "^6.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.2.0",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "sideEffects": true,
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
